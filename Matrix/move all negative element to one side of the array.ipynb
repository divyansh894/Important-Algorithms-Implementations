   "source": [
    "arr=[-12, 11, -13, -5, 6, -7, 5, -3, -6]\n",
    "np=0\n",
    "pp=len(arr)-1\n",
    "while np<=pp:\n",
    "    if arr[np]<0:\n",
    "        np+=1\n",
    "    elif arr[pp]>0:\n",
    "        pp-=1\n",
    "    else:\n",
    "        arr[np],arr[pp]=arr[pp],arr[np]\n",
    "        np+=1\n",
    "        pp-=1\n",
    "print(arr)"
   ]
